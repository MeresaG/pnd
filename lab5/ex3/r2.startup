ip addr add 100.90.0.100/16 dev eth0
ip route add default via 100.90.0.1

apt install -y ipsec-tools racoon

# Flush the SAD and SPD
flush;
spdflush;

# AH SAs using 128 bit long keys
add 100.60.0.100 100.90.0.100 ah 0x200 -A hmac-md5 
0xc0291ff014dccdd03874d9e8e4cdf3e6;

add 100.90.0.100 100.60.0.100 ah 0x300 -A hmac-md5 
0x96358c90783bbfa3d7b196ceabe0536b;


# ESP SAs using 192 bit long keys (168 + 24 parity)
add 100.60.0.100 100.90.0.100 esp 0x201 -E 3des-cbc 
0x7aeaca3f87d060a12f4a4487d5a5c3355920fae69a96c831;
add 100.90.0.100 100.60.0.100 esp 0x301 -E 
3des-cbc 0xf6ddb555acfd9d77b03ea3843f2653255afe8eb5573965df;

# Security policies
spdadd 100.60.0.100 100.90.0.100 any -P in ipsec 
esp/transport//require 
ah/transport//require;

spdadd 100.90.0.100 100.60.0.100 any -P out ipsec 
esp/transport//require 
ah/transport//require;