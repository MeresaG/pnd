iface eth0 inet6 manual
iface eth0 inet6 static
  address 2001:db8:fede:1::3
  netmask 64
  autoconf 0
  accept_ra 0

iface eth0 inet6 static
  address fe80::3
  netmask 10
  autoconf 0
  accept_ra 0
iface eth1 inet6 manual
iface eth1 inet6 static
  address 2001:db8:fede:3::1
  netmask 64
  autoconf 0
  accept_ra 0

iface eth1 inet6 static
  address fe80::3
  netmask 10
  autoconf 0
  accept_ra 0

